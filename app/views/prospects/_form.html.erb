<%= form_for @prospect do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <%= f.label :phone %>
  <%= f.text_field :phone %>
  <%= f.label :description %>
  <%= f.text_area :description %>
  <%= f.label :budget %>
  <%= f.text_field :budget %>  
  <%= f.label :timetable %>
  <%= f.text_field :timetable %>
  <%= f.label :referral %>
  <%= f.text_field :referral %>
  <%= f.submit %>
<% end %>